<h1> Edit Shop Profile </h1>

<%= form_for @bookshop do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :zipcode %>
    <%= f.number_field :zipcode %>
    <%= f.label :phone_number %>
    <%= f.number_field :phone_number %>
    <%= f.submit "Update Shop Profile" %>

<% end %>